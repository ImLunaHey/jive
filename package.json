{
  "name": "jive",
  "scripts": {
    "postinstall": "npx patch-package",
    "build": "npx tsup ./src/main.ts --external redlock",
    "dev": "npm run build -- --onSuccess=\"MAINTENCE_MODE=true node dist/main.cjs\" --watch",
